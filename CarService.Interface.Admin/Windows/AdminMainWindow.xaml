<Window x:Class="CarService.Interface.Admin.Windows.AdminMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarService.Interface.Admin.Windows"
        mc:Ignorable="d"
        Title="MainWindowV2" Height="600" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="60"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <ToolBar Grid.Column="0" x:Name="toolBar" HorizontalAlignment="Left" Width="69" Margin="-10,0,0,4" />
        <Grid Grid.Column="1" x:Name="GridForUserControls">
            <Button x:Name="btAllUsers" HorizontalAlignment="Left" Margin="43,10,0,0" VerticalAlignment="Top" Width="75">
                <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="TextBlock"/>
            </Button>
            <TextBox x:Name="tbFirstUser" HorizontalAlignment="Left" Height="23" Margin="141,10,0,0" TextWrapping="Wrap" Text="first" VerticalAlignment="Top" Width="120"/>

        </Grid>

    </Grid>
</Window>
